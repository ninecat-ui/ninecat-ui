{
  "presets": [
    ["@babel/preset-env", {
      "modules": "commonjs",
      "targets": {
        "node": "current",
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }]
  ],
  "plugins": [
    "jsx-v-model", 
    "transform-vue-jsx",
    ["@babel/plugin-proposal-decorators",{"legacy": true}]
  ]
}
